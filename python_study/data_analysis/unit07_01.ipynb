    "%matplotlib inline\n",
    "import matplotlib.pyplot as plt\n",
    "import csv\n",
    "f=open('age.csv')\n",
    "data=csv.reader(f)\n",
    "result=[]\n",
    "\n",
    "for row in data :\n",
    "    if '신도림' in row[0]:\n",
    "        for i in row[3:]:\n",
    "            result.append(int(i))\n",
    "plt.style.use('ggplot')           \n",
    "plt.plot(result)\n",
    "plt.show()"
   ]
